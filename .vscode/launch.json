{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch",
        "type": "node",
        "runtimeVersion": "16.13.1",
        "request": "launch",
        "program": "${workspaceRoot}/index.js",
        "stopOnEntry": false,
        "args": [],
        "cwd": "${workspaceRoot}",
        "preLaunchTask": null,
        "runtimeExecutable": "nodemon",
        "runtimeArgs": ["--nolazy", "--experimental-specifier-resolution=node"],
        "console": "internalConsole",
        "sourceMaps": true,
        "outFiles": [],
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ]
      }
    ]
  }